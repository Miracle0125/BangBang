<?xml version="1.0" encoding="utf-8"?>
<com.yzx.bangbang.view.indvInfo.AcceptedAssignItem xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/accepted_asm_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="15dp"
        android:text="Title"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/title"
        android:layout_below="@+id/title"
        android:layout_marginTop="5dp"
        android:text="2000.1.1" />

    <TextView
        android:id="@+id/symbol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/date"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/date"
        android:text="Â¥" />

    <TextView
        android:id="@+id/price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/date"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/symbol"
        android:text=""
        android:textSize="20sp" />

    <FrameLayout
        android:background="@color/gray"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/date"
        android:layout_marginTop="15dp" />

</com.yzx.bangbang.view.indvInfo.AcceptedAssignItem>